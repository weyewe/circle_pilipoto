
<div class="span9 content">
  
  <%= create_guide("Select the Project", 
        "Manage your project: adding pictures, viewing progress").html_safe %>
  
  <%= create_breadcrumb(@breadcrumbs).html_safe %>
  
  

  <div class="row-fluid">
    <!-- <div class="span12"> -->

      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th>Project Name</th>
            <th>Pictures</th>
            <th>Selection Quota</th>
            <th>Selected Pictures</th>
            <th>Selection DONE</th>
            <th>Revision</th>
            <th>Approved</th>
            <th>Finalized?</th>
            <th>Delete</th>
            
          </tr>
        </thead>
        <tbody>
          <% @projects.each do |project| %>
            <%= render :partial => "projects/project_to_be_managed", :locals => {
              :project => project
            }%>
          <% end %>
          


        </tbody>
      </table>



    <!-- </div> -->   <!--/span12-->
  </div><!--/row-->


  <div class=" hidden" id="close-entry">
    <%= form_tag  finalize_project_url  , :method => :post  do  %>
      <%= hidden_field_tag 'entry_id' , ''  %>
    <% end %>
  </div>
  
  <div class=" hidden" id="flexible-entry">
    <%= form_tag  finalize_project_url  , :method => :post  do  %>
      <%= hidden_field_tag 'flexible_entry_id' , ''  %>
    <% end %>
  </div>
 

</div><!--/span-->












