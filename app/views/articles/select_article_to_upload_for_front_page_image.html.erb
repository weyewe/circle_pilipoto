
<div class="span9 content">
  
  <%= create_guide("Front Page Display", 
        "Select Article to add Front Page Display").html_safe %>
  
  <%= create_breadcrumb(@breadcrumbs).html_safe %>
  
  

  <div class="row-fluid">
    <!-- <div class="span12"> -->

      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th>Article Name</th>
            <th style="text-align:center;">Front Page Picture</th>
            <th  style="text-align:center;">Front Page Stock</th> 
          </tr>
        </thead>
        <tbody>
          <% @articles.each do |article| %>
            <%= render :partial =>"articles/article_front_page", :locals => {
              :article => article
            }%>
          <% end %>
          


        </tbody>
      </table>



    <!-- </div> -->   <!--/span12-->
  </div><!--/row-->


  <div class=" hidden" id="close-entry">
    <%= form_tag  create_article_from_project_url  , :method => :post  do  %>
      <%= hidden_field_tag 'entry_id' , ''  %>
    <% end %>
  </div>
  
  <!-- <div class=" hidden" id="article-publisher">
     <%#= form_tag  execute_publish_project_based_article_url  , :method => :post  do  %>
       <%#= hidden_field_tag 'entry_publish_id' , ''  %>
       <%#= hidden_field_tag 'publisher_action' , ''  %>
     <%# end %>
   </div> -->
 

</div><!--/span-->

<a id="header_pane"></a>
 <!--/ header -->

 <div id="slide_info_container" class="post_item">
   <!--Slide captions displayed here-->
   <div id="slidecaption"><h3></h3></div>
   <div class="clear"></div>
   <!--Slide description displayed here-->
   <div id="slidedescription" class="entry"></div>
   <div id="slide_info_toggle"></div>
 </div>
 










