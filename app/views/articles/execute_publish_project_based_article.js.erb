$(document).ready(function(){
  $("#entry_<%= @article.id %>").hideLoading();
  
  <% new_row = escape_javascript(
    render :partial => "projects/article_from_project", :locals => {
      :project => @project,
      :article => @article
    }
  ) %>


  $("#entry_row_<%= @project.id %>").replaceWith('<%= new_row %>');
  
});