	padding-top:15px;
	background:url(<%= asset_data_uri 'front_page/divider_line_black.gif' %> ) bottom repeat-x;}

.divider{
	background:url(<%= asset_data_uri 'front_page/divider_line_black.gif' %> ) bottom repeat-x;}

.footer .divider{
	background:url(<%= asset_data_uri 'front_page/divider_line_black.gif' %> ) bottom repeat-x;}

